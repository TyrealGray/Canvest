{
  "name": "canvest",
  "private": true,
  "version": "0.0.1",
  "description": "A unit testing framework for HTML5 canvas",
  "main": "index.js",
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "scripts": {
    "release": "npm run build && lerna version --no-push",
    "build": "node ./script/prebuild.js && lerna exec -- npm run build",
    "dev": "lerna run dev --scope @canvest/canvest-cli",
    "iall": "lerna exec -- npm i",
    "lall": "lerna link --force-local",
    "lint": "lerna exec -- npm run lint",
    "log": "auto-changelog",
    "test": "lerna exec -- npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyrealGray/Canvest.git"
  },
  "keywords": [
    "canvas",
    "unit-test"
  ],
  "author": "TyrealGray",
  "license": "AFL-3.0",
  "bugs": {
    "url": "https://github.com/TyrealGray/Canvest/issues"
  },
  "homepage": "https://github.com/TyrealGray/Canvest#readme",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "auto-changelog": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "core-js": "3.6.0",
    "eslint": "9.39.1",
    "lerna": "^9.0.3",
    "prettier": "^2.0.4"
  }
}
